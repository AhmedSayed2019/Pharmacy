<?xml version="1.0" encoding="utf-8"?>
<layer-list
xmlns:android="http://schemas.android.com/apk/res/android">

<item android:height="@dimen/normal_height"
    android:width="50dp">
    <shape android:shape="rectangle">
        <solid android:color="@color/colorPrimary"/>
        <corners android:bottomLeftRadius="@dimen/large_margin"
            android:topLeftRadius="@dimen/large_margin"/>
        <padding
            android:bottom="@dimen/small_margin"
            android:top="@dimen/small_margin"
            android:left="@dimen/small_margin"
            android:right="@dimen/small_margin" />
    </shape>



</item>

<item android:drawable="@drawable/ic_person_white_24dp"
    android:width="35dp"
    android:height="35dp"
    android:gravity="center">



</item>

</layer-list>